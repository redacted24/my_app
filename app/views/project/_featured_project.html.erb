<a href="<%= url %>" class="no-color-a">
  <div class="mb-l exp-item">
    <div class="exp-name">
      <%= name %>
    </div>
    <div class="exp-description">
      <%= description %>
    </div>
    <div class="tag-container">
      <% for tag in tags do %>
        <div class="tag">
          <%= tag %>
        </div>
      <% end %>
    </div>
  </div>
</a>
